model User {
  id           String   @id @default(uuid())
  name         String?
  first_name   String?
  last_name    String?
  password     String
  email        String   @unique
  phone_number Int?
  role         Role     @default(user)
  img          String?
  categories   Category[] 
  createdAt    DateTime @default(now())
  is_active    Boolean  @default(false)
  updatedAt DateTime @updatedAt
}


